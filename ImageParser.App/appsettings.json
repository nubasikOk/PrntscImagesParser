{
  "GrabbedFilesFolder": "D:\\outputs-grabbed",
  "StorageAccount": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "grabbed-images"
  },
  "Logging": {
      "ApplicationInsights": {
        "Instrumentationkey": "ikey",
        "LogLevel": {
          "Default": "Warning",
          "Microsoft": "Warning"
        }
      },
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning"
      }
    },
    "NLog": {
      "internalLogLevel": "Debug",
      "extensions": {
        "NLog.Web.AspNetCore": {
          "assembly": "NLog.Web.AspNetCore"
        }
      },
      "targets": {
        "logconsole": {
          "type": "Console",
          "layout": "${longdate} [${threadid}] ${uppercase:${level}} ${callsite:includeNamespace=false} - ${message} ${exception}"
        }
      },
      "rules": [
        {
          "logger": "*",
          "minLevel": "Debug",
          "writeTo": "logconsole"
        },
        {
          "logger": "Microsoft.*",
          "maxLevel": "Info",
          "final": "true"
        }
      ]
    }
  }
